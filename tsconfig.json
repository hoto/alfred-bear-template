{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "workflow",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": [
    "src/alfred/templates.ts",
    "src/alfred/compile.ts",
    "src/alfred/open.ts"
  ]
}
